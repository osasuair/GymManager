<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane fx:id="newMemberPane" prefHeight="679.0" prefWidth="1366.0" style="-fx-background-color: white;"
            xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.gymmanagementsystem.NewMember">
    <Button layoutX="1326.0" mnemonicParsing="false" onAction="#close" prefHeight="42.0" prefWidth="48.0">
        <graphic>
            <ImageView fitHeight="34.0" fitWidth="33.0" pickOnBounds="true" preserveRatio="true">
                <Image url="@../images/close.png"/>
            </ImageView>
        </graphic>
    </Button>
    <AnchorPane layoutX="239.0">
        <AnchorPane layoutY="69.0">
            <Label layoutX="248.0" prefHeight="73.0" prefWidth="394.0" text="New Member" textFill="#000000be">
                <font>
                    <Font name="System Bold" size="48.0"/>
                </font>
                <graphic>
                    <ImageView fitHeight="82.0" fitWidth="82.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../images/newMember.png"/>
                    </ImageView>
                </graphic>
            </Label>
            <Label layoutX="1.0" layoutY="115.0" text="Member ID:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <Label layoutY="152.0" prefHeight="17.0" prefWidth="63.0" text="Name:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <Label fx:id="idLabel" layoutX="266.0" layoutY="115.0" text="M000001" textAlignment="RIGHT"
                   textFill="RED">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <TextField fx:id="nameTextField" layoutY="175.0" prefHeight="26.0" prefWidth="316.0"/>
            <Label layoutY="213.0" prefHeight="17.0" prefWidth="112.0" text="Mobile Number:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <TextField fx:id="mobileTextField" layoutY="236.0" prefHeight="26.0" prefWidth="316.0"/>
            <Label layoutY="275.0" prefHeight="17.0" prefWidth="63.0" text="Email:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <TextField fx:id="emailTextField" layoutY="298.0" prefHeight="26.0" prefWidth="316.0"/>
            <Label layoutX="1.0" layoutY="339.0" prefHeight="17.0" prefWidth="63.0" text="Gender:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <ComboBox fx:id="genderComboBox" layoutX="1.0" layoutY="362.0" prefHeight="26.0" prefWidth="316.0"
                      visibleRowCount="3"/>
            <Label layoutY="404.0" prefHeight="17.0" prefWidth="112.0" text="Parent 1 Name:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <TextField fx:id="parent1TextField" layoutY="427.0" prefHeight="26.0" prefWidth="316.0"/>
            <Label layoutX="573.0" layoutY="150.0" prefHeight="17.0" prefWidth="112.0" text="Parent 2 Name:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <TextField fx:id="parent2TextField" layoutX="573.0" layoutY="173.0" prefHeight="26.0" prefWidth="316.0"/>
            <Label layoutX="573.0" layoutY="213.0" prefHeight="17.0" prefWidth="112.0" text="Gym Time:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <ComboBox fx:id="gymTimeComboBox" layoutX="573.0" layoutY="236.0" prefHeight="26.0" prefWidth="316.0"/>
            <Label layoutX="573.0" layoutY="277.0" prefHeight="17.0" prefWidth="112.0" text="SIN Number:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <TextField fx:id="sinTextField" layoutX="573.0" layoutY="298.0" prefHeight="26.0" prefWidth="316.0"/>
            <Label layoutX="573.0" layoutY="339.0" prefHeight="17.0" prefWidth="112.0" text="Age:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <TextField fx:id="ageTextField" layoutX="573.0" layoutY="362.0" prefHeight="26.0" prefWidth="316.0"/>
            <Label layoutX="573.0" layoutY="404.0" prefHeight="17.0" prefWidth="112.0" text="Monthly Fee:">
                <font>
                    <Font name="System Bold" size="12.0"/>
                </font>
            </Label>
            <TextField fx:id="monthlyFeeTextField" layoutX="573.0" layoutY="427.0" prefHeight="26.0"
                       prefWidth="316.0"/>
            <Button layoutX="1.0" layoutY="503.0" mnemonicParsing="false" onAction="#clear" text="Clear">
                <graphic>
                    <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../images/reset.png"/>
                    </ImageView>
                </graphic>
            </Button>
            <Button layoutX="814.0" layoutY="503.0" mnemonicParsing="false" onAction="#save" text="Save">
                <graphic>
                    <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../images/save.png"/>
                    </ImageView>
                </graphic>
            </Button>
        </AnchorPane>
    </AnchorPane>
</AnchorPane>
